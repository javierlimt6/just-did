{
  "manifest_version": 3,
  "name": "Just Did",
  "version": "1.0",
  "description": "Prompts you at intervals to quickly log your tasks",
  "permissions": ["storage", "notifications", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}